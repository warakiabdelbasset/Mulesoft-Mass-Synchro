    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="lockToken">
          <xs:annotation>
            <xs:documentation>The Lock Token from the message to Abandon. This represents a UUID for a message.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/azure-service-bus-messaging" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CompleteMessageType" substitutionGroup="mule:abstract-operator" name="complete-message">
    <xs:annotation>
      <xs:documentation>Complete a message received from in a flow with a message listener in the current source (this is the same as an acknowledge)</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CompleteMessageType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="lockToken">
          <xs:annotation>
            <xs:documentation>The Lock Token from the message to ACK. This represents a UUID for a message.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/azure-service-bus-messaging" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="ReceiveType" substitutionGroup="mule:abstract-operator" name="receive">
    <xs:annotation>
      <xs:documentation>This operation receives a message from a specific queue or subscription. In this version it only supports the Immediate Receive Mode (RECEIVEANDDELETE).</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="ReceiveType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="optional" name="outputMimeType">
          <xs:annotation>
            <xs:documentation>The mime type of the payload that this operation outputs.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="outputEncoding">
          <xs:annotation>
            <xs:documentation>The encoding of the payload that this operation outputs.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="required" name="destinationName">
          <xs:annotation>
            <xs:documentation>"The name of the destination from which messages will be received"</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" default="NONE" name="subscriptionName">
          <xs:annotation>
            <xs:documentation>The name of the subscription from which messages will be received</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="target">
          <xs:annotation>
            <xs:documentation>The name of a variable on which the operation's output will be placed</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" default="#[payload]" name="targetValue">
          <xs:annotation>
            <xs:documentation>An expression that will be evaluated against the operation's output and the outcome of that expression will be stored in the target variable</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/azure-service-bus-messaging" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="ReceiveBatchType" substitutionGroup="mule:abstract-operator" name="receive-batch">
    <xs:annotation>
      <xs:documentation>This operation receives a maximum of maxMessageCount messages from a specific queue or subscription. In this version it only supports the Immediate Receive Mode (RECEIVEANDDELETE).</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="ReceiveBatchType">
