                  <xs:attribute type="mule:expressionString" use="optional" name="correlationId"></xs:attribute>
                  <xs:attribute type="mule:expressionString" use="optional" name="sessionId"></xs:attribute>
                  <xs:attribute type="mule:expressionString" use="optional" name="replyTo"></xs:attribute>
                  <xs:attribute type="mule:expressionString" use="optional" name="replyToSessionId"></xs:attribute>
                  <xs:attribute type="mule:expressionString" use="optional" name="label"></xs:attribute>
                  <xs:attribute type="mule:expressionString" use="optional" name="partitionKey"></xs:attribute>
                  <xs:attribute type="mule:expressionLong" use="optional" name="timeToLive"></xs:attribute>
                  <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/azure-service-bus-messaging" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="SECONDS" name="timeToLiveTimeUnit"></xs:attribute>
                  <xs:attribute type="mule:expressionDateTime" use="optional" name="scheduledEnqueueTimeUtc"></xs:attribute>
                  <xs:attribute type="mule:expressionMap" use="optional" name="properties"></xs:attribute>
                </xs:extension>
              </xs:complexContent>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
