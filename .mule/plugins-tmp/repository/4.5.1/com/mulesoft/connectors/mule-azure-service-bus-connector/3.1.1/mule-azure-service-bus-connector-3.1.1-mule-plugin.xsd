  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/azure-service-bus-messaging" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="SendType" substitutionGroup="mule:abstract-operator" name="send">
    <xs:annotation>
      <xs:documentation>Sends a message to a Queue or Topic</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="SendType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element type="xs:string" minOccurs="0" maxOccurs="1" name="body">
            <xs:annotation>
              <xs:documentation>The body of this message</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="properties">
            <xs:annotation>
              <xs:documentation>Map of user application properties set on the message</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="property">
                  <xs:complexType>
                    <xs:attribute type="mule:expressionString" use="required" name="key"></xs:attribute>
                    <xs:attribute type="mule:expressionString" use="required" name="value"></xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="destinationName">
          <xs:annotation>
            <xs:documentation>The destination name to which the message will be sent.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="messageId">
          <xs:annotation>
            <xs:documentation>The message identifier is an application-defined value that uniquely identifies the message and its payload</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/azure-service-bus-messaging" type="org.mule.runtime.extension.api.runtime.parameter.OutboundCorrelationStrategyEnumType" use="optional" name="sendCorrelationStrategy"></xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="correlationId">
          <xs:annotation>
            <xs:documentation>Allows an application to specify a context for the message for the purposes of correlation</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="sessionId">
          <xs:annotation>
            <xs:documentation>The session identifier for a session-aware entity</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="replyTo">
          <xs:annotation>
            <xs:documentation>The name of the destination to which the message reply will be sent</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="replyToSessionId">
          <xs:annotation>
            <xs:documentation>The ID of the Session to be tied with the message</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="label">
          <xs:annotation>
            <xs:documentation>Enables the application to indicate the purpose of the message to the receiver</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="partitionKey"></xs:attribute>
        <xs:attribute type="mule:expressionLong" use="optional" name="timeToLive"></xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/azure-service-bus-messaging" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="SECONDS" name="timeToLiveTimeUnit">
          <xs:annotation>
            <xs:documentation>The Time unit for the Write Concern Timeout</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionDateTime" use="optional" name="scheduledEnqueueTimeUtc"></xs:attribute>
        <xs:attribute type="mule:expressionMap" use="optional" name="properties">
          <xs:annotation>
            <xs:documentation>Map of user application properties set on the message</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/azure-service-bus-messaging" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="SendMessageBatchType" substitutionGroup="mule:abstract-operator" name="send-message-batch">
