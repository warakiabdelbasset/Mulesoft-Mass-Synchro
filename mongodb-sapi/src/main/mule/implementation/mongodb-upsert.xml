		<ee:transform doc:name="Transform Message" doc:id="a75ffbb7-cf7e-4934-993a-245a088de7ea" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/java
---

	test : "testabdo"
]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<try doc:name="Try" doc:id="a30e781d-e7c5-430e-83d2-fbc6c5278911" >
